<h1>Cadastre</h1>

<form action="/books/insertbook" method="post">
    <div class="form-control">
        <label for="title">Titulo</label>
        <input type="text" name="title" placeholder="Digite um titulo"/>
    </div>
    <div class="form-control">
        <label for="page">Numero de Paginas</label>
        <input type="number" name="page" placeholder="numeros de paginas"/>
    </div>
    <input type="submit" value="cadastrar"/>
</form>